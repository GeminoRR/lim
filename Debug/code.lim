class stack<T>
	let content = new list<T>

	func push(value:T)
		content.add(value)

	func remove:T
		//return content.pop(-1)

	func str:str
		let result = ""
		let first = true
		for i in content
			if first
				result = i.repr()
				first = false
			else
				result = result + ", " + i.repr()
		return "<" + result + "]"

func main
	let s = new stack<str>
	s.push("hello")
	s.push("world")
	puts(s.str())